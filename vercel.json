{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "web-dashboard/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/",
      "dest": "/web-dashboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web-dashboard/$1"
    }
  ]
}
